{
  "name": "data-dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"cross-env NODE_ENV=development electron electron/main.js\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "@codemirror/lang-sql": "6.8.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "^6.20.0",
    "@element-plus/icons-vue": "^2.3.1",
    "ace-builds": "^1.43.1",
    "codemirror": "^5.65.19",
    "codemirror-editor-vue3": "^2.8.0",
    "d3": "^7.9.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.7.3",
    "monaco-editor": "^0.52.2",
    "monaco-editor-vue3": "^0.1.10",
    "mysql2": "^3.14.2",
    "node-sql-parser": "^4.11.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-codemirror6": "^1.3.18",
    "vue-router": "^4.3.0",
    "vue3-ace-editor": "^2.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-reloader": "^1.2.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.example.data_dev",
    "productName": "DataDevPlatform",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**",
      "electron/**",
      "node_modules/**",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "d:/工作/data_dev/bee.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "debug": {
    "element-plus": "^2.3.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  }
}
